{
  "name": "cat-energy-20",
  "private": true,
  "description": "Личный проект «Кэт энерджи» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "9.8.4",
    "browser-sync": "2.26.7",
    "gulp": "^4.0.2",
    "gulp-less": "4.0.1",
